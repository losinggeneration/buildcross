--- newlib-1.17.1/libgloss/libsysbase/close.c	1970-01-01 01:00:00.000000000 +0100
+++ newlib-1.17.1-ppc/libgloss/libsysbase/close.c	2009-06-16 23:32:27.000000000 +0100
@@ -0,0 +1,42 @@
+#include "config.h"
+#include <_ansi.h>
+#include <_syslist.h>
+#include <stdio.h>
+#include <errno.h>
+
+#include "handle_manager.h"
+
+#ifdef REENTRANT_SYSCALLS_PROVIDED
+//---------------------------------------------------------------------------------
+int
+_DEFUN(_close_r,(ptr,fileDesc),
+		   struct _reent *ptr _AND
+           int fileDesc) {
+//---------------------------------------------------------------------------------
+#else
+//---------------------------------------------------------------------------------
+int _DEFUN(_close,(fileDesc),
+           int fileDesc) {
+//---------------------------------------------------------------------------------
+	struct _reent *ptr = _REENT;
+#endif
+	int ret = -1;
+	unsigned int dev = 0;
+	unsigned int fd = -1;
+
+	if(fileDesc!=-1) {
+
+		__handle *handle = __get_handle(fileDesc);
+
+		if ( handle != NULL) {
+			dev = handle->device;
+			fd = (unsigned int)handle->fileStruct;
+
+			if(devoptab_list[dev]->close_r)
+				ret = devoptab_list[dev]->close_r(ptr,fd);
+
+			__release_handle(fileDesc);
+		}
+	}
+	return ret;
+}
